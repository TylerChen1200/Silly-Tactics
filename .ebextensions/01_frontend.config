option_settings:
  aws:elasticbeanstalk:container:nodejs:
    NodeCommand: "npm start"
    ProxyServer: nginx
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production

container_commands:
  01_install_frontend:
    command: "npm run install-frontend"
  02_build_frontend:
    command: "npm run build-frontend"